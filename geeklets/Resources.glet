	<string>top -l 1 -n 0 | awk '/Load Avg/ {print "AVG: " $3" "$4" "$5 };/CPU usage/ {print "CPU: " $3" / "$5" / "$7 };/PhysMem/ {print "RAM: " $2" / "$6}'
